package me.draeprogs.customcommands;

import org.bukkit.command.CommandExecutor;
import org.bukkit.plugin.java.JavaPlugin;

import me.draeprogs.customcommands.commands.Commands;

public class Main extends JavaPlugin{
	
	public void onEnable() {
		System.out.println("(!) Draeprogs plugin Enabled");
		this.getCommand("discord").setExecutor((CommandExecutor)new Commands());
	}
	
	public void onDisable() {
		System.out.println("(!) Draeprogs plugin Disabled");
	}

}
