commands:
    discord:
        description: Shows the server's Discord link
    website:
        description: Shows the server's Website link
    store:
        description: Shows the server's Webstore link
        aliases: [webstore]
    fortunatecommands:
        description: Shows the author of the plugin.
        usage: "/<command> v" Shows the version
        aliases: [dpauthor, draeprogs, fcreload, fc]
