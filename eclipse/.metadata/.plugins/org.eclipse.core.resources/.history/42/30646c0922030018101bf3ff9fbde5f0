package me.draeprogs.customcommands.commands;

import org.bukkit.command.Command;
import org.bukkit.command.CommandExecutor;
import org.bukkit.command.CommandSender;
import org.bukkit.entity.Player;

import me.draeprogs.customcommands.Main;
import net.md_5.bungee.api.ChatColor;

public class website implements CommandExecutor{
	
Main plugin;
	
	public Commands(Main passesPlugin) {
		this.plugin = passesPlugin;
	}
	
	@Override
	public boolean onCommand(CommandSender sender, Command cmd, String label, String[] args) {
		Player player = (Player) sender;
		
		if (sender instanceof Player) {
			player.sendMessage(ChatColor.translateAlternateColorCodes('&', "&5&lWebsite link &8&l// &d&lhttps://linuxpvp.enjin.com/"));
		}
		else {
			sender.sendMessage("Discord link // https://linuxpvp.enjin.com/");
		}
		
		return false;
	}

}
